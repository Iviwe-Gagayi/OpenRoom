datasource db {
  provider = "postgresql"

}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       String    @id
  name     String?
  bookings Booking[]
}

model Room {
  id       String    @id @default(cuid())
  name     String    @unique
  bookings Booking[]
}

model Booking {
  id       String @id @default(cuid())
  slotTime String
  userId   String
  user     User   @relation(fields: [userId], references: [id])
  roomId   String
  room     Room   @relation(fields: [roomId], references: [id])

  @@unique([roomId, slotTime])
}